<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="jj.framework.persistence" type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
  </configSections>

  <jj.framework.persistence
    contextType="Xml"
    location="."
    modelAssembly="JJ.Models.SetText"
    mappingAssembly="JJ.Models.SetText.Persistence.Xml">
    <repositoryAssemblies>
      <repositoryAssembly>JJ.Models.SetText.Persistence.Xml</repositoryAssembly>
      <repositoryAssembly>JJ.Models.SetText.Persistence</repositoryAssembly>
    </repositoryAssemblies>
  </jj.framework.persistence>
  
  <appSettings>
    <add key="AppServiceUrl" value="http://localhost:6371/SetTextWithSyncAppService.svc" />
    <add key="SynchronizationTimerIntervalInMilliseconds" value="7000" />
    <add key="CheckServiceAvailabilityTimeoutInMilliseconds" value="300" />
  </appSettings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>