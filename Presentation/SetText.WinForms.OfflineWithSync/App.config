<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="jj.framework.data" type="JJ.Framework.Configuration.ConfigurationSectionHandler, JJ.Framework.Configuration" />
  </configSections>

  <jj.framework.data
    contextType="Xml"
    location="."
    modelAssembly="JJ.Data.SaveText"
    mappingAssembly="JJ.Data.SaveText.Xml">
    <repositoryAssemblies>
      <repositoryAssembly>JJ.Data.SaveText.Xml</repositoryAssembly>
      <repositoryAssembly>JJ.Data.SaveText.DefaultRepositories</repositoryAssembly>
    </repositoryAssemblies>
  </jj.framework.data>
  
  <appSettings>
    <add key="AppServiceUrl" value="http://localhost:6371/SaveTextWithSyncAppService.svc" />
    <add key="SynchronizationTimerIntervalInMilliseconds" value="7000" />
    <add key="CheckServiceAvailabilityTimeoutInMilliseconds" value="300" />
  </appSettings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>